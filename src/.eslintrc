{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-key": 1,
    "react/react-in-jsx-scope": 1,
    "no-console": 1,
    "object-shorthand": 0,
    "class-methods-use-this": 0,
    "react/prop-types": [
      0
    ],
    "max-len": [
      1,
      140,
      2,
      {
        "ignoreComments": true
      }
    ]
  },
  "globals": {
    "window": true,
    "document": true
  },
  "extends": [
    "airbnb-base",
    "plugin:react/recommended"
  ]
}