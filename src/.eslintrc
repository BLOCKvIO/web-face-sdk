{
  "parser": "babel-eslint",
  "rules": {
    "no-console": 1,
    "object-shorthand": 0,
    "class-methods-use-this": 0,
    "max-len": [
      1,
      140,
      2,
      {
        "ignoreComments": true
      }
    ]
  },
  "globals": {
    "window": true,
    "document": true
  },
  "extends": [
    "airbnb-base"
  ]
}